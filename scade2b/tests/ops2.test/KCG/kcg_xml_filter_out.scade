function Op_cast(x : int; y : real) returns (r : int)
var
  L1 : int;
  L2 : int;
let
  L1 = real x;
  L2 = L1 + y;
  r = int L2;
tel

function Op_sharp(x : bool; y : bool) returns (r : bool)
var
  L1 : int;
  L2 : int;
let
  L1 = x;
  L2 = y;
  r = #(x, y, L1, L2);
tel

function Op_transpose(x : int^3^2) returns (r : int^2^3)
let
  r = transpose (x; 1; 2);
tel
