function Op_plus(xx : int; yy : int)
  returns (rr : int)
var
  zz : int;
let
  zz = xx + yy;
  rr = zz;
tel

function Op_minus(xx : int; yy : int)
  returns (rr : int)
var
  zz : int;
let
  zz = xx - yy;
  rr = zz;
tel

function Op_cmp (aa: int; bb: int; cc: int; dd: int; ee: int; ff: int)
  returns (rr : bool)
var
  zz : bool;
let
  zz = ((aa <= bb) or (aa >= bb))
   and ((cc <  dd) or (cc >  dd))
   and ((ee =  ff) or (ee <> ff));
  rr = zz;
tel

function Op_unops(aa: int)
  returns (rr: int)
var
 bb: int;
 cc: int;
let
  bb = -aa;
  cc = bb;
  rr = bb + cc;
tel

--@ bin_arith_op/slash/check
function Op_div(aa: int; bb: int)
  returns (rr: int)
let
  rr = aa div bb;
tel

function Op_not(aa:bool)
  returns (rr:bool)
let
  rr = not aa;
tel

function Op_mul(aa: int; bb: int)
  returns (rr: int)
let
  rr = aa * bb;
tel

function Op_mod(aa: int; bb: int)
  returns (rr: int)
let
  rr = aa mod bb;
tel
